{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsup src/index.ts --format cjs",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup src/index.ts --format cjs --watch --onSuccess \"node dist/index.js\"",
    "lint": "tsc --noEmit && TIMING=1 eslint \"src/**/*.ts*\"",
    "start": "node dist/index.js",
    "test": "jest --coverage --json --outputFile=\"coverage-report.json\"",
    "test:jest": "jest --watch"
  },
  "jest": {
    "preset": "jest-presets/jest/node",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "~/(.*)": "<rootDir>/src/$1"
    },
    "resetMocks": false,
    "collectCoverage": true,
    "coverageProvider": "v8"
  },
  "dependencies": {
    "@line/bot-sdk": "^7.5.2",
    "axios": "^1.1.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "mongoose": "^6.7.2",
    "morgan": "^1.10.0",
    "qs": "^6.11.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@jest-mock/express": "^2.0.1",
    "@swc/core": "^1.3.18",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.22",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.2",
    "@types/qs": "^6.9.7",
    "@types/supertest": "^2.0.12",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "jest-mock-axios": "4.7.0-beta2",
    "jest-presets": "*",
    "mockingoose": "^2.15.2",
    "supertest": "^6.2.4",
    "ts-jest": "^26.5.6",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  }
}
